<script type="text/javascript">
  // <![CDATA[
const warehouseInfo = document.querySelectorAll('.store-finder__button--warehouse-info');
  const warehouseInfoModal = document.querySelector('.store-finder__modal');
  warehouseInfo.forEach((insideBox) => {
      insideBox.addEventListener('click', (event) => {
      if(warehouseInfoModal.classList.contains('hidden')) warehouseInfoModal.classList.remove('hidden');
      else  warehouseInfoModal.classList.add('hidden');
      });
  });

  const modalClose = document.querySelectorAll('.store-finder__modal-close');
  modalClose.forEach((insideBox) => {
      insideBox.addEventListener('click', (event) => {
      warehouseInfoModal.classList.add('hidden');
      });
  });

  window.addEventListener('click', function(e){
      if (document.querySelector('.store-finder__modal') == e.target){
          warehouseInfoModal.classList.add('hidden');
      }
  });

  // Toggle between tour and map
  const mapTourButton = document.querySelectorAll('.map-tour-button');
  mapTourButton.forEach((insideBox) => {
      insideBox.addEventListener('click', (event) => {
      if(insideBox.closest('.store-finder__item').querySelector('.store-tour').classList.contains('hidden')) {
          insideBox.closest('.store-finder__item').querySelector('.store-tour').classList.remove('hidden');
          insideBox.closest('.store-finder__item').querySelector('.utility-store-map').classList.add('hidden');
          insideBox.classList.remove('active');
          insideBox.innerHTML = "View Store Location";
      } else {
          insideBox.closest('.store-finder__item').querySelector('.store-tour').classList.add('hidden');
          insideBox.closest('.store-finder__item').querySelector('.utility-store-map').classList.remove('hidden');
          insideBox.classList.add('active');
          insideBox.innerHTML = "Take Virtual Tour";
      }
      });
  });
